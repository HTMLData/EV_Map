{
  "name": "EV_Map",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:https": "vite --host 0.0.0.0 --port 3000",
    "dev:mobile": "node start-mobile.js",
    "build": "vite build",
    "preview": "vite preview",
    "preview:https": "vite preview --host 0.0.0.0 --port 4173 --https",
    "clean": "rm -rf node_modules/.vite .vite dist && echo 'Vite缓存已清理'",
    "dev:clean": "npm run clean && npm run dev",
    "generate-certs": "node scripts/generate-certs.js",
    "generate-certs:win": "generate-certs.bat",
    "generate-certs:unix": "./generate-certs.sh",
    "serve:https": "npm run build && npm run preview:https"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "pinia": "^3.0.3",
    "vant": "^4.9.21",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "terser": "^5.44.0",
    "vite": "^5.4.10"
  }
}
